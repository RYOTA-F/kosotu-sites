{
  "repository": "https://github.com/vercel/examples.git",
  "license": "MIT",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "dev:carrerlog": "turbo run dev --scope=kosotu-carrerlog",
    "dev:devlog": "turbo run dev --scope=kosotu-devlog",
    "start": "turbo run start",
    "sb:ui": "cd packages/ui && pnpm run storybook",
    "lint": "turbo run lint",
    "clean": "turbo run clean",
    "test:ui": "cd packages/ui && pnpm run test"
  },
  "devDependencies": {
    "eslint": "^8.28.0",
    "eslint-config-turbo": "latest",
    "husky": "^8.0.0",
    "turbo": "^1.8.3"
  }
}
